{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "baseUrl": "../",
        "paths": {
            "*": [
                "node_modules/*",
            ],
        },
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
    },
}